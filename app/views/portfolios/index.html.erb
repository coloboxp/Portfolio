<h1>Portfolio Items</h1>

<!--a href="portfolios/new">New portfolio</a-->
<%= link_to "Create new portfolio", new_portfolio_path %>
<% @portfolioItems.each do |item| %>
  <p><h2><%= link_to item.title, portfolio_path(item.id) %></h2></p>
  <p><%=item.subtitle%></p>
  <p><%=item.body%></p>
  <%= image_tag item.thumb_image if !item.thumb_image.nil? %>
  <%= link_to "Edit", edit_portfolio_path(item.id) %>
<% end %>